{
    "name": "webfont-vscode",
    "displayName": "Webfont",
    "description": "Import and download fonts from Fontstorage.com collection.",
    "version": "1.2.0",
    "icon": "images/logo.jpg",
    "publisher": "Fontstorage",
    "homepage": "https://github.com/WebFont/webfont_vscode/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/WebFont/webfont_vscode.git"
    },
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "web font",
        "fonts"
    ],
    "activationEvents": [
        "onCommand:webfont"
    ],
    "main": "./out/extension.js",
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        },
        "virtualWorkspaces": true
    },
    "contributes": {
        "commands": [
            {
                "command": "webfont",
                "title": "Webfont"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorLangId == css",
                    "command": "webfont"
                },
                {
                    "when": "editorLangId == html",
                    "command": "webfont"
                },
                {
                    "when": "resourceExtname == .sass",
                    "command": "webfont"
                },
                {
                    "when": "resourceExtname == .scss",
                    "command": "webfont"
                },
                {
                    "when": "resourceExtname == .less",
                    "command": "webfont"
                },
                {
                    "when": "resourceExtname == .styl",
                    "command": "webfont"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^9.1.1",
        "@types/node": "^16.11.33",
        "@types/vscode": "^1.68.0",
        "mocha": "^10.0.0",
        "typescript": "^4.7.4",
        "tslint": "^5.8.0"
    },
    "dependencies": {
        "axios": "^0.27.2"
    }
}
