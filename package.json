{
    "name": "webfont-vscode",
    "displayName": "webfont-vscode",
    "description": "Plugin for importing fonts from https://fontstorage.com/.",
    "version": "0.0.1",
    "publisher": "WebFont",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.importFont",
        "onCommand:extension.downloadFont"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.importFont",
                "title": "Webfont: Import Font"
            },
            {
                "command": "extension.downloadFont",
                "title": "Webfont: Download Font"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "axios": "^0.18.0"
    }
}
